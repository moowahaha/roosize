{
  "imageSource": {
    "type": "filesystem",
    "path": "./spec/fixtures"
  },
  "listenPort": 8080,
  "connectionLimit": 200,
  "cacheControl": {
    "inboundTTL": 5,
    "outboundTTL": 3600
  },
  "requestDefaults": {
    "paddingColor": {
      "value": "6600AA",
      "allowOverride": true
    },
    "strategy": {
      "value": "stretch",
      "allowOverride": true
    }
  },
  "limits": {
    "range": {
      "maximum": {
        "width": 1600,
        "height": 1200
      },
      "minimum": {
        "width": 400,
        "height": 300
      }
    },
    "sizes": [
      {
        "width": 200,
        "height": 400
      },
      {
        "width": 640,
        "height": 480
      },
      {
        "width": 800,
        "height": 600
      },
      {
        "width": 3200,
        "height": 2400
      }
    ]
  }
}

